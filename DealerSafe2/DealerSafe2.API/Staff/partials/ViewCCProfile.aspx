<div class="page-header">
    <h1 class="blue">
        <i class="icon-tag"></i>
        Profile
    <small>
        <i class="icon-double-angle-right"></i>
        view profile details
    </small>
    </h1>
</div>

<div id="user-profile-1" class="user-profile row">

    <div class="col-xs-12 col-sm-12">

        <div style="clear: both"></div>
        <br />
        <br />
        <div class="profile-user-info profile-user-info-striped">

            <div class="profile-info-row">
                <div class="profile-info-name">Client </div>
                <div class="profile-info-value">{{entity.ClientName}}</div>
            </div>

            <div class="profile-info-row">
                <div class="profile-info-name">Name </div>
                <div class="profile-info-value">{{entity.Name}}</div>
            </div>

            <div class="profile-info-row">
                <div class="profile-info-name">Profile Type </div>
                <div class="profile-info-value">{{entity.ProfileType}}</div>
            </div>

            <div class="profile-info-row">
                <div class="profile-info-name">Priority </div>
                <div class="profile-info-value">{{entity.Priority}}</div>
            </div>

        </div>

        <!-- /widget-body -->
    </div>

</div>



